    </dependency>
    <dependency>
      <groupId>org.scalatestplus</groupId>
      <artifactId>scalacheck-1-17_2.12</artifactId>
      <version>3.2.16.0</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>scalacheck_2.12</artifactId>
          <groupId>org.scalacheck</groupId>
        </exclusion>
